   "source": [
    "# 1. Print multiplication table of 24 and 50 upto 10"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "0bf5e71c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "24*1=24\n",
      "24*2=48\n",
      "24*3=72\n",
      "24*4=96\n",
      "24*5=120\n",
      "24*6=144\n",
      "24*7=168\n",
      "24*8=192\n",
      "24*9=216\n",
      "24*10=240\n",
      "_________\n",
      "50*1=50\n",
      "50*2=100\n",
      "50*3=150\n",
      "50*4=200\n",
      "50*5=250\n",
      "50*6=300\n",
      "50*7=350\n",
      "50*8=400\n",
      "50*9=450\n",
      "50*10=500\n",
      "_________\n"
     ]
    }
   ],
   "source": [
    "nums = [24,50]\n",
    "for n in nums:\n"        #Using For loop to iterate through the nums list to get 24 and 50,
    "    i = 1\n",
    "    while i<=10:\n",
    "        print(f'{n}*{i}={n*i}')\n",
    "        i += 1\n",
    "    print('_________')"
   ]
  },
